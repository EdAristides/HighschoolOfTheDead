<p id="notice"><%= notice %></p>

<h1>Listing Professores</h1>

<table>
  <thead>
    <tr>
      <th>Siape</th>
      <th>Cpf</th>
      <th>Nome</th>
      <th>Email</th>
      <th>Telefone</th>
      <th>Dtnascimento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @professores.each do |professore| %>
      <tr>
        <td><%= professore.siape %></td>
        <td><%= professore.cpf %></td>
        <td><%= professore.nome %></td>
        <td><%= professore.email %></td>
        <td><%= professore.telefone %></td>
        <td><%= professore.dtnascimento %></td>
        <td><%= link_to 'Show', professore %></td>
        <td><%= link_to 'Edit', edit_professore_path(professore) %></td>
        <td><%= link_to 'Destroy', professore, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Professore', new_professore_path %>
